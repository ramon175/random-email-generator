{
  "display": "Node LTS",
  "_version": "20.1.0",
  "compilerOptions": {
    "lib": ["es2023"],
    "types": ["node"],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16",
    "noUncheckedIndexedAccess": true,
    "outDir": "dist"
  },
  "include": ["src"]
}
